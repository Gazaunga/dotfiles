Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

XTerm*foreground:blue
XTerm*background:yellow

XTerm*reverseVideo:   	    on
xterm*VT100.geometry:       80x24
xterm-big*VT100.geometry:   80x48
xterm-small*VT100.geometry: 80x10

oneko*tora: true

emacs*Font: 8x13             ! emacs with 8x13 font, 80 columns
emacs*geometry: 80x48+0+0    !   wide, 48 columns high

URxvt*termName:rxvt-256color
URxvt*transparent: true
URxvt*depth: 32
URxvt*shading: 10
URxvt*saveLines: 12000
URxvt*font: xft:OhSnap :pixelsize=8:antialias=true:hinting=true
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*borderLess:           false
URxvt*inheritPixmap:	    true
URxvt.urlLauncher: uzbl
URxvt*geometry: 98x24

URxvt*background:        #262729
URxvt*foreground:        #dddddd
URxvt*cursorColor:       #8c817e
URxvt*colorBD:           #585653
URxvt*colorUL:           #6c615e

